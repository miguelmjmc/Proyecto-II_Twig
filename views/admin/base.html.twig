|---------------------------------------------------------|
-->
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

    <!-- Main Header -->
    <header class="main-header">

        <!-- Logo -->
        <a href="#" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><i class="fa fa-plus"></i></span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>SISCATEL</b></span>
        </a>

        <!-- Header Navbar -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>
            <!-- Navbar Right Menu -->
            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    <!-- User Account Menu -->
                    <li class="dropdown user user-menu">
                        <!-- Menu Toggle Button -->
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <!-- The user image in the navbar-->
                            <img src="data:{{ array.admin.adminImgType }}; base64,{{ array.admin.adminImg }}" class="user-image" alt="Imagen de usuario">
                            <!-- hidden-xs hides the username on small devices so only the image appears. -->
                            <span class="hidden-xs">{{ array.admin.name }} {{ array.admin.lastName }}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- The user image in the menu -->
                            <li class="user-header">
                                <img src="data:{{ array.admin.adminImgType }}; base64,{{ array.admin.adminImg }}"
                                     class="img-circle" alt="Imagen de usuario">
                                <p>{{ array.admin.name }} {{ array.admin.lastName }}</p>
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="#" class="btn btn-default btn-flat">Perfil</a>
                                </div>
                                <div class="pull-right">
                                    <a href="index.php?logout=true" class="btn btn-default btn-flat">Salir</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">

        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">

            <!-- Sidebar user panel (optional) -->
            <div class="user-panel">
                <div class="pull-left image">
                    <img src="data:{{ array.admin.adminImgType }}; base64,{{ array.admin.adminImg }}" class="img-circle" alt="Imagen de usuario">
                </div>
                <div class="pull-left info">
                    <p>{{ array.admin.name }} {{ array.admin.lastName }}</p>
                    <!-- Status -->
                    <a href="#"><i class="fa fa-circle text-success"></i> En linea</a>
                </div>
            </div>

            <!-- Sidebar Menu -->
            <ul class="sidebar-menu">
                <li class="header"></li>
                <!-- Optionally, you can add icons to the links -->
                <li class="active"><a href="index.php"><i class="fa fa-home"></i> <span>Inicio</span></a></li>
                <li><a href="index.php?link=adminProduct"><i class="fa fa-th"></i> <span>Productos</span></a></li>
                <li><a href="index.php?link=adminVehicle"><i class="fa fa-car"></i> <span>Vehiculos</span></a></li>
                <li><a href="index.php?link=adminUser"><i class="fa fa-group"></i> <span>Usuarios</span></a></li>
                <li><a href="index.php?link=adminReport"><i class="fa fa-area-chart"></i> <span>Reportes</span></a></li>
                <li><a href="index.php?link=adminHistory"><i class="fa fa-history"></i> <span>Historial</span></a></li>
                <li><a href="index.php?link=adminConfig"><i class="fa fa-cogs"></i> <span>Ajustes</span></a></li>
                <li><a href="index.php?link=adminHelp"><i class="fa fa-book"></i> <span>Documentaci√≥n</span></a></li>
            </ul>
            <!-- /.sidebar-menu -->
        </section>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                {% block pageTitle %}

                {% endblock %}
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Inicio</a></li>
                {% block breadcrumb %}
                {% endblock %}
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <!-- Your Page Content Here -->
            {% block main %}

            {% endblock %}

        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <!-- Main Footer -->
    <footer class="main-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="copyright">
                        <p>&copy; 2017. Todos los Derechos Reservados. Codificado por <a href="#">Miguel, Oel,
                                Carlos.</a>
                        </p>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="footer-card-icon">
                        <img src="{{ pathAsset }}img/tech.png"
                             alt="Tecnologias utilizadas para el desarrollo del sistema"
                             class="tech">
                    </div>
                </div>
            </div>
        </div>
    </footer>

</div>
<!-- ./wrapper -->

